package javaapplication32;

public class Pez extends Animal{
    @override
    public void dormir (){
        System.out.println("Duermo flotando");
    }
    @override
    public void comunicarse (){
        System.out.println("glu gluu");
    }
    
    public void comer(){
        System.out.println("Me gusta el agua");
    }
}    
}
